<h1>Already a member? Login here.</h1>
<% if @user.errors.any? %>
    <div> There are errors. </div> 
    <ul>
    <% @user.errors.full_messages.each do |message|%>
        <li> <%= message %> </li>
    <% end %>

    </ul>
<% end %>

<%= form_with url: "/sessions" do |f| %>
    <%= f.label "E-Mail:" %>
    <%= f.email_field :email %><br/>
    <%= f.label "Password:" %>
    <%= f.password_field :password %><br/> 
    <%= f.submit "Login" %>
<% end %>
<%= button_to "Log in with Google", '/auth/google_oauth2'  %>